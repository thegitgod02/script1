local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Tab = Window:CreateTab("Universal", 4483362458)
    local Section = Tab:CreateSection("Walkspeed")
    local WalkSpeedSlider = Tab:CreateSlider({
       Name = "Walkspeed",
       Range = {0, 1000},
       Increment = 10,
       Suffix = "Walkspeed",
       CurrentValue = 16,
       Flag = "WSLIDER", 
       Callback = function(ws)
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = ws
       end,
    })
    local WalkSpeedButton = Tab:CreateButton({
        Name = "Reset Walkspeed",
        Callback = function()
            WalkSpeedSlider:Set(16)
        end,
    })
    local Section = Tab:CreateSection("Jumppower")
    local JumpPowerSlider = Tab:CreateSlider({
       Name = "Jumppower",
       Range = {0, 1000},
       Increment = 10,
       Suffix = "Jumppower",
       CurrentValue = 50,
       Flag = "JPSLIDER", 
       Callback = function(jp)
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = jp
       end,
    })
    local JumpPowerButton = Tab:CreateButton({
        Name = "Reset Jumppower",
        Callback = function()
            JumpPowerSlider:Set(50)
        end,
    })
    local Section = Tab:CreateSection("Main scripts")
    local IYBUTTON = Tab:CreateButton({
      Name = "Load infinite yield",
      Callback = function()
         loadstring(game:HttpGet("https://raw.githubusercontent.com/edgeiy/infiniteyield/master/source", true))()
      end,
   })
    local Section = Tab:CreateSection("Other")
    local AntiAFKButton = Tab:CreateButton({
       Name = "Antiafk",
       Callback = function()
          loadstring(game:HttpGet("https://raw.githubusercontent.com/evxncodes/mainroblox/main/anti-afk", true))()
       end,
    })
